{
  "name": "micronextblog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently yarn:dev:*",
    "dev:next": "next dev",
    "dev:css": "yarn run build:css -w",
    "build": "yarn run build:css && next build",
    "build:css": "CSS_ENV=build postcss css/index.css --config postcss.config.js -o css/bundle.css",
    "start": "next start"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "clsx": "^1.1.1",
    "next": "^9.3.3",
    "postcss-preset-env": "^6.7.0",
    "raw-loader": "^4.0.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "tailwindcss": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "concurrently": "^5.2.0",
    "postcss-cli": "^7.1.1",
    "typescript": "^3.9.7"
  }
}
