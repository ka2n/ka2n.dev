{
  "name": "micronextblog",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "concurrently yarn:dev:*",
    "dev:next": "next dev",
    "dev:css": "yarn run build:css -w",
    "now-build": "yum install libuuid-devel libmount-devel && cp /lib64/{libuuid,libmount,libblkid}.so.1 node_modules/canvas/build/Release/ && yarn build",
    "build": "NODE_ENV=production yarn run build:css && next build",
    "build:css": "CSS_ENV=build postcss css/index.css --config postcss.config.js -o css/bundle.css",
    "start": "next start"
  },
  "workspaces": {
    "nohoist": [
      "konva-node",
      "canvas"
    ]
  },
  "dependencies": {
    "@neojp/tailwindcss-line-clamp-utilities": "^1.0.1",
    "axios": "^0.19.2",
    "axios-cache-adapter": "^2.5.0",
    "canvas": "2.6.1",
    "clsx": "^1.1.1",
    "hast-util-to-text": "^2.0.0",
    "image-data-uri": "^2.0.1",
    "immer": "^7.0.7",
    "konva": "^7.0.4",
    "konva-node": "^0.11.1",
    "next": "^9.3.3",
    "postcss-preset-env": "^6.7.0",
    "raw-loader": "^4.0.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "rehype-parse": "^7.0.1",
    "rehype-plaintext": "^1.0.0",
    "rehype-stringify": "^8.0.0",
    "remark-html": "^12.0.0",
    "remark-parse": "^8.0.3",
    "remark-react": "^7.0.1",
    "remark-toc": "^7.0.0",
    "tailwindcss": "^1.6.0",
    "unified": "^9.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "concurrently": "^5.2.0",
    "postcss-cli": "^7.1.1",
    "typescript": "^3.9.7"
  }
}
