{
  "name": "micronextblog",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "fetch-site-config": "node lib/fetch-site-config.js",
    "dev": "concurrently yarn:dev:*",
    "dev:next": "yarn fetch-site-config && env TZ='utc' next dev",
    "now-build": "yum install libuuid-devel libmount-devel && cp /lib64/{libuuid,libmount,libblkid}.so.1 node_modules/canvas/build/Release/ && yarn build",
    "build": "yarn fetch-site-config && next build",
    "start": "next start"
  },
  "workspaces": {
    "nohoist": [
      "konva-node",
      "canvas"
    ]
  },
  "dependencies": {
    "@neojp/tailwindcss-line-clamp-utilities": "^1.0.1",
    "axios": "^0.21.1",
    "axios-cache-adapter": "^2.5.0",
    "canvas": "2.6.1",
    "clsx": "^1.1.1",
    "copy-to-clipboard": "^3.3.1",
    "hast-util-to-text": "^2.0.0",
    "html-react-parser": "^1.2.7",
    "image-data-uri": "^2.0.1",
    "immer": "^9.0.3",
    "konva": "^8.0.4",
    "konva-node": "^0.11.1",
    "luxon": "^1.27.0",
    "next": "^11.0.0",
    "next-seo": "^4.26.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-string-replace": "^0.4.4",
    "rehype-parse": "^7.0.1",
    "rehype-plaintext": "^1.0.0",
    "rehype-split-paragraph": "^0.1.1",
    "rehype-stringify": "^8.0.0",
    "typesafe-utils": "^1.13.0",
    "unified": "^9.1.0"
  },
  "devDependencies": {
    "@types/concurrently": "^6.2.0",
    "@types/luxon": "^1.27.0",
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "autoprefixer": "^10.2.6",
    "concurrently": "^6.2.0",
    "lost": "^8.3.1",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^2.2.2",
    "typescript": "^4.3.4"
  }
}
