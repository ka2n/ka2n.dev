    "@types/node": "^16.7.10",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/rison": "^0.0.7",
    "autoprefixer": "^10.3.4",
    "concurrently": "7.3.0",
    "lost": "^8.3.1",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "7.7.2",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.3.5"
  }
