{
  "name": "micronextblog",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "fetch-site-config": "node lib/fetch-site-config.js",
    "dev": "concurrently yarn:dev:*",
    "dev:next": "yarn fetch-site-config && next dev",
    "now-build": "yum install libuuid-devel libmount-devel && cp /lib64/{libuuid,libmount,libblkid}.so.1 node_modules/canvas/build/Release/ && yarn build",
    "build": "yarn fetch-site-config && next build",
    "start": "next start"
  },
  "workspaces": {
    "nohoist": [
      "konva-node",
      "canvas"
    ]
  },
  "dependencies": {
    "@neojp/tailwindcss-line-clamp-utilities": "^1.0.1",
    "axios": "^0.19.2",
    "axios-cache-adapter": "^2.5.0",
    "canvas": "2.6.1",
    "clsx": "^1.1.1",
    "copy-to-clipboard": "^3.3.1",
    "hast-util-to-text": "^2.0.0",
    "image-data-uri": "^2.0.1",
    "immer": "^7.0.7",
    "konva": "^7.0.4",
    "konva-node": "^0.11.1",
    "next": "^11.0.0",
    "next-seo": "^4.26.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rehype-parse": "^7.0.1",
    "rehype-plaintext": "^1.0.0",
    "rehype-stringify": "^8.0.0",
    "remark-html": "^12.0.0",
    "remark-parse": "^8.0.3",
    "remark-react": "^7.0.1",
    "remark-toc": "^7.0.0",
    "unified": "^9.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "autoprefixer": "^10.2.6",
    "concurrently": "^5.2.0",
    "postcss": "^8.3.5",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^2.2.2",
    "typescript": "^4.3.4"
  }
}
